@model OceanNex.Models.FeedBackPostagem

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>FeedBackPostagem</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StatusFeedBackPostagem" class="control-label"></label>
                <select asp-for="StatusFeedBackPostagem" class="form-control">
                    <option value="Positivo">Positivo</option>
                    <option value="Negativo">Negativo</option>
                </select>
                <span asp-validation-for="StatusFeedBackPostagem" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DescricaoFeedBackPostagem" class="control-label"></label>
                <input asp-for="DescricaoFeedBackPostagem" class="form-control" />
                <span asp-validation-for="DescricaoFeedBackPostagem" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ContaId" class="control-label"></label>
                <select asp-for="ContaId" class ="form-control" asp-items="ViewBag.ContaId"></select>
            </div>
            <div class="form-group">
                <label asp-for="PostagemId" class="control-label"></label>
                <select asp-for="PostagemId" class ="form-control" asp-items="ViewBag.PostagemId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
