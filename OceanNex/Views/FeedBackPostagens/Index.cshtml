@model IEnumerable<OceanNex.Models.FeedBackPostagem>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StatusFeedBackPostagem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescricaoFeedBackPostagem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Conta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Postagem)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StatusFeedBackPostagem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescricaoFeedBackPostagem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Conta.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Postagem.Bibliografia)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FeedBackPostagemId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FeedBackPostagemId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FeedBackPostagemId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
